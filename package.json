{
  "name": "web-app-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc": "pnpm exec tsc --noEmit",
    "prettier:check": "pnpm exec prettier --check .",
    "prettier:fix": "pnpm exec prettier --write .",
